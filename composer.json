{
    "name": "calatoiu/cnpcui-rule",
    "description": "A Laravel validation rule to validate CNPCUI numbers",
    "version": "1.0.3",
    "keywords": [
        "laravel",
        "validation",
        "rule",
        "calatoiu",
        "cnpcui-rule"
    ],
    "homepage": "https://github.com/calatoiu/cnpcui-rule",
    "license": "MIT",
    "authors": [
        {
            "name": "Calatoiu",
            "email": "calatoiu@gmail.com",        
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/validation": ">=10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "calatoiu\\CnpcuiRule\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit tests"
    }
